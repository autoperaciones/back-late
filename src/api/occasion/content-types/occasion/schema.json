{
  "kind": "collectionType",
  "collectionName": "occasions",
  "info": {
    "singularName": "occasion",
    "pluralName": "occasions",
    "displayName": "Occasion",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "occasion_id": {
      "type": "customField",
      "regex": "^[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}$",
      "customField": "plugin::field-uuid.uuid"
    },
    "occasion_name": {
      "type": "text",
      "required": true,
      "unique": true
    },
    "products": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product.product",
      "mappedBy": "occasions_name"
    },
    "category": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "activo": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "occasion_date": {
      "type": "date"
    }
  }
}
